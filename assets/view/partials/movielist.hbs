<div class="movie-container">
  <div class="movie-list">
    {{#each data}}
    <a class="movie-list-item" href="/details/{{meta.id}}">
      <img src="{{ image }}" class="movie-poster" alt="{{ meta.title }}">
      <section class="movie-details">
        <span class="movie-title">{{ meta.title }}</span>
        <span class="movie-duration">{{ meta.id }}</span>
      </section>

    </a>
    {{/each }}
  </div>
</div>
